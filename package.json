{
  "name": "@chouchouji/vite-plugin-component-placeholder",
  "version": "0.0.0",
  "description": "A vite plugin to generate componentPlaceholder config for miniprogram",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "release": "pnpm build && vr release"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "vite",
    "plugin",
    "weixin",
    "miniprogram",
    "componentPlaceholder",
    "subPackages"
  ],
  "author": "chouchouji <1305974212@qq.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@varlet/release": "^0.4.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.7"
  }
}